{
  "name": "jgent",
  "displayName": "JGent",
  "description": "VS Code Custom Agent with Local LLM",
  "version": "0.0.1",
  "main": "./src/extension.js",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:jgent.runAgent",
    "onCommand:jgent.openSettings"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jgent.runAgent",
        "title": "JGent: Run Agent"
      },
      {
        "command": "jgent.openSettings",
        "title": "JGent: Open Settings"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
